<div class="container">
    <div class="row">
        <div class="col">
            <h1>{{title}}</h1>
        </div>
    </div>
    {{#each blogPosts}}
        <div class="row">
            <div class="col my-3">
                <h4 class="text-white">{{{this.title.rendered}}}</h4>
                <p class="text-white my-3">{{{this.excerpt.rendered}}}</p>
            </div>
        </div>
    {{/each}}
</div>


