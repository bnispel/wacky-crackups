<div class="col">
    <h1>Characters</h1>
</div>

<div class="row justify-content-center characters">
    {{#each characters}}
        <div class="col-6 col-md-4">
            <img src="{{this.image_url}}">
            <p class="text-white name my-3">{{this.name}}</p>
        </div>
    {{/each}}
</div>

{{>calltoaction}}
